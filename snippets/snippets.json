{
	"mflow: node generator": {
		"prefix": "nodes.metadata",
		"body": [ 
			"metadata:",
			"    type: action",
			"    title: '$2'",
			"    description: '$3'",
			"    script: ",
			"      id: $4",
			"  id: '$1'"
		],
		"description": "node generator"
	},
	"mflow: node selector generator": {
		"prefix": "nodes.selector.metadata",
		"body": [ 
			"metadata:",
			"    type: selector",
			"    title: selector",
			"  id: '$1'"
		],
		"description": "node generator"
	},
	"mflow: edge generator": {
		"prefix": "edges.source",
		"body": [
			"source: '$1'",
			"  target: '$2'",
			"  metadata:",
			"    binding:",
			"    - property: $3",
			"      value: $4",
			"      type: '${5:string|number|array|object}'"
		],
		"description": "edge generator"
	},
	"mflow: edge selector generator": {
		"prefix": "edges.source.selector",
		"body": [
			"source: '$1'",
			"  target: '$2'",
			"  metadata:",
			"    filters:",
			"      property: $3",
			"      operator: '${4:>|==|<|!=|>=|<=}'",
			"      value: $5",
			"    binding:",
			"    - property: $6",
			"      value: $7",
			"      type: '${8:string|number|array|object}'"
		],
		"description": "edge selector generator"
	},
	"mflow: cron trigger": {
		"prefix": "type cron trigger",
		"body": [
			"type: cron",
			"  data: '${1:0 * * * *}'"
		],
		"description": "cron trigger"
	},
	"mflow: event trigger": {
		"prefix": "type event trigger",
		"body": [
			"type: event",
			"  data: '${1:event+id}::${2:version}'"
		],
		"description": "event trigger"
	},
	"mflow: action property": {
		"prefix": "action property",
		"body": [
			"property: $1",
			"  value: ${2}",
			"  type: '${3:string|number|array|object}'"
		],
		"description": "binding action property"
	}
}